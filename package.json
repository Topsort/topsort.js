{
  "name": "@topsort/sdk",
  "version": "0.3.5",
  "description": "The official Topsort SDK for TypeScript and JavaScript",
  "packageManager": "bun@1.3.1",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "author": "MÃ¡rcio Barbosa <marcio@topsort.com>",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Topsort/topsort.js"
  },
  "bugs": {
    "url": "https://github.com/Topsort/topsort.js/issues"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "dist/index.d.cjs",
    "dist/index.d.ts",
    "dist/index.js",
    "dist/index.cjs",
    "dist/index.global.js",
    "package.json"
  ],
  "scripts": {
    "build": "bunup",
    "test:e2e": "bun run build && playwright test",
    "format": "biome check",
    "format:fix": "biome check --write",
    "prepare": "lefthook install",
    "serve:e2e": "bun run ./e2e/server.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.1",
    "@playwright/test": "1.56.1",
    "@types/bun": "1.3.1",
    "bunup": "0.15.12",
    "lefthook": "2.0.0",
    "msw": "2.11.2",
    "typescript": "5.9.3"
  },
  "keywords": [
    "topsort",
    "sdk",
    "javascript",
    "typescript"
  ]
}
