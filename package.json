    "serve:e2e": "bun run ./e2e/server.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@playwright/test": "1.46.0",
    "@types/bun": "1.1.6",
    "lefthook": "1.7.12",
    "msw": "2.3.5",
    "tsup": "8.2.4",
    "typescript": "5.5.4"
  }
